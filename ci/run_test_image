if [ $CI_COMMIT_BRANCH == "master" ];
    then TAG="latest";
    else TAG="$CI_COMMIT_BRANCH";
fi

docker run -d --name $TAG gcr.io/krossr/client:${TAG}
docker exec $TAG sh -c "ls -la"
docker cp $TAG:/dist $CI_PROJECT_DIR/dist
docker cp $TAG:/node_modules $CI_PROJECT_DIR/node_modules
docker stop $TAG