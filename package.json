        "@angular/core": "^15.0.3",
        "@angular/forms": "^15.0.3",
        "@angular/material": "^15.0.3",
        "@angular/platform-browser": "^15.0.3",
        "@angular/platform-browser-dynamic": "^15.0.3",
        "@angular/router": "^15.0.3",
        "@krossr/api": "^0.0.7",
        "@types/lodash": "^4.14.163",
