        "codelyzer": "^5.1.2",
        "cross-env": "^7.0.2",
        "jasmine-core": "^3.6.0",
        "jasmine-spec-reporter": "^5.0.2",
        "karma": "^5.1.1",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage-istanbul-reporter": "^3.0.3",
        "karma-jasmine": "^3.3.1",
        "karma-jasmine-html-reporter": "^1.5.4",
        "protractor": "~7.0.0",
        "puppeteer": "^5.2.1",
