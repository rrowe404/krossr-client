{
    "name": "krossr-client",
    "version": "0.0.0",
    "scripts": {
        "clean": "rimraf dist",
        "ng": "ng",
        "build": "by-node-env",
        "build:development": "ng build",
        "build:production": "ng build --prod=true",
        "test": "cross-env NODE_ENV=test ng test",
        "test:development": "ng test",
        "e2e": "ng e2e",
        "lint": "ng lint",
        "start": "npm run clean && npm run devServer",
        "devServer": "ng serve --host 0.0.0.0 --disableHostCheck"
    },
    "private": true,
    "dependencies": {
